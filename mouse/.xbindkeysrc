###########################
# xbindkeys configuration #
###########################

#keystate_numlock = enable
#keystate_capslock = enable
#keystate_scrolllock= enable

# Mouse wheel left
 "xte 'keydown Alt_L' 'key Left' 'keyup Left' 'keyup Alt_L'"
 b:6

# Mouse wheel right
 "xte 'keydown Alt_L' 'key Right' 'keyup Right' 'keyup Alt_L'"
 b:7

# Mouse bottom button
 "xte 'keydown Control_L' 'keydown Super_L' 'key Down' 'keyup Down' 'keyup Super_L' 'keyup Control_L'"
 b:8

# Mouse top button
 "xte 'keydown Control_L' 'keydown Super_L' 'key Up' 'keyup Up' 'keyup Super_L' 'keyup Control_L'"
 b:9

##################################
# End of xbindkeys configuration #
##################################
